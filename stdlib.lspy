(def {nil} {})
(def {true} 1)
(def {false} 0)

(def {fun} (\ {f b}
    {def (head f) (\ (tail f) b)}))

(fun {unpack f l}
    {eval (join (list f) l)})

(fun {pack f & xs} {f xs})

(def {curry} unpack)
(def {uncurry} pack)

(fun {do & li}
    {if (== li nil)
        {nil}
        {last li}})

(fun {let b}
    {((\ {_} b) ())})

(fun {not x} {- 1 x})
(fun {or x y} {+ x y})
(fun {and x y} {* x y})
